<h1>People#index</h1>
<p>Find me in app/views/people/index.html.erb</p>

<table>
  <tr>
    <th>Name</th>
    <th>Data</th>
    <th></th>
  </tr>
 
 <% @people.each do |person| %>
  <tr>
    <td><%= person.name %></td>
    <td><%= person.data %></td>
    <td><%= link_to "Show", person %> </td>
  </tr>
<% end %>
 
</table>
 
<br>

<%= link_to 'Add Person', 'people/new' %>
